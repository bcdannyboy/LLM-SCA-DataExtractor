# API Keys
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Database Configuration
SCAUDIT_DB_KEY=your-encryption-key-here
SCAUDIT_DB_PATH=~/.scaudit/audit.db

# Default Models
SCAUDIT_DEFAULT_TARGET=gpt-4o
SCAUDIT_DEFAULT_JUDGES=gpt-4o,claude-3-opus

# Rate Limiting
SCAUDIT_REQUESTS_PER_MINUTE=60
SCAUDIT_TOKENS_PER_MINUTE=150000
SCAUDIT_MAX_CONCURRENT=10

# Model Parameters
SCAUDIT_TEMPERATURE=0.7
SCAUDIT_MAX_TOKENS=1000

# Filter Thresholds
SCAUDIT_FILTER_MIN_LENGTH=20
SCAUDIT_FILTER_MIN_SPECIAL_RATIO=0.15
SCAUDIT_FILTER_MIN_ENTROPY=2.0

# Similarity Search
SCAUDIT_SIMILARITY_THRESHOLD=0.95
SCAUDIT_VECTOR_BACKEND=faiss