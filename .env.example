# LLM-SCA-DataExtractor Demo Environment Configuration
# Copy this file to .env and fill in your API keys for full LLM testing

# OpenAI API Configuration
OPENAI_API_KEY=your-openai-api-key-here

# Anthropic API Configuration  
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Optional: Database encryption key for SCAudit
SCAUDIT_DB_KEY=your-32-character-encryption-key-here

# Optional: Default models for testing
SCAUDIT_DEFAULT_TARGET=gpt-4o
SCAUDIT_DEFAULT_JUDGES=gpt-4o,claude-3-opus

# Demo Configuration
DEMO_MODE=offline  # Options: offline, online
DEMO_LOG_LEVEL=INFO